<odoo>
    <data noupdate="1">
        <record id="cron_expire_jobs" model="ir.cron">
            <field name="name">Expire Jobs</field>
            <field name="model_id" ref="model_dumuc_job"/>
            <field name="state">code</field>
            <field name="code">model.cron_expire_jobs()</field>
            <field name="interval_type">hours</field>
            <field name="interval_number">1</field>
        </record>
        <record id="cron_subscription_monthly" model="ir.cron">
            <field name="name">Subscription Monthly Renew</field>
            <field name="model_id" ref="model_dumuc_company_subscription"/>
            <field name="state">code</field>
            <field name="code">model._cron_monthly_renew()</field>
            <field name="interval_type">days</field>
            <field name="interval_number">1</field>
        </record>
    </data>
</odoo>
