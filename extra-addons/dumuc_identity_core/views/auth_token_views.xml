<odoo>

    <record id="view_dumuc_auth_token_tree" model="ir.ui.view">
        <field name="model">dumuc.auth.token</field>
        <field name="arch" type="xml">
            <tree>
                <field name="user_id"/>
                <field name="scope"/>
                <field name="issued_at"/>
                <field name="expire_at"/>
                <field name="is_active"/>
            </tree>
        </field>
    </record>

    <record id="action_dumuc_auth_token" model="ir.actions.act_window">
        <field name="name">Token phiên đăng nhập</field>
        <field name="res_model">dumuc.auth.token</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_dumuc_auth_token_tree"/>
    </record>

</odoo>
